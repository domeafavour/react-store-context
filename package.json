    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/react": ">=16.8.0",
    "@types/react-dom": ">=16.8.0",
    "happy-dom": "^16.5.3",
    "react": ">=16.8.0",
